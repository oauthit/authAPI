a.btn.btn-social.btn-facebook(ng-class='classes'
  ng-click='OauthButtons.loginOauth("facebook/debtee")')
  i.fa.fa-facebook
  |  Connect with Facebook

a.btn.btn-social.btn-google(ng-class='classes'
  ng-click='OauthButtons.loginOauth("google/debtee")')
  i.fa.fa-google-plus
  |  Connect with Google+

a.btn.btn-social.btn-twitter(ng-class='classes'
  ng-click='OauthButtons.loginOauth("twitter")')
  i.fa.fa-twitter
  |  Connect with Twitter

a.btn.btn-social.btn-openid(ng-class='classes'
  ng-click='OauthButtons.loginOauth("sms/sms")')
    i.glyphicon.glyphicon-phone
    |  Connect with mobile number
